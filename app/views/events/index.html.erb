<div class="wrapper-main">
  <h2>AGENDA SPECTACLES ET STAGES</h2>
  <span class="subtitle">Retrouvez ici tous nos prochains événements !</span>
  <% if current_user %>
    <%= link_to "Créer un nouvel évènement", (new_event_path), class: :button, id: :new_event %>
  <% end %>
  <div class='events-wrapper'> 
    <% @events.each do |event| %>
      <%= render "shared/event", event: event%>
    <% end %>
  </div>
</div>

