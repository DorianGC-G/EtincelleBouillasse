<%= link_to biography_path(biographie) do %>
  <div class="clown-card-container">
    <div class="clown-thumb">
      <img src=<%= biographie.poster %>>
    </div>
    <div class="name">
      <h3><%= biographie.name %></h3>
      <span><%= biographie.clown_name %></span>
    </div>
  </div>
<% end %>